@model AlbumDto



	<h4>Edit Album</h4>
<form class="form-horizontal col-lg-8" method="post">
	<input asp-for="Id" hidden />
	<div class="form-group">
		<label class="" asp-for="Artist"></label>
		<input class="form-control" type="text" asp-for="Artist" />
		<span asp-validation-for="Artist" class="text-danger"></span>

	</div>
	<div class="form-group">

		<label class="" asp-for="Title"></label>
		<input class="form-control" type="text" asp-for="Title"></input>
		<span asp-validation-for="Title" class="text-danger"></span>
	</div>
	<div class="form-group">

		<label class="form-label" asp-for="ReleaseDate"></label>
		<input disabled class="form-control" type="date" asp-for="ReleaseDate"></input>

	</div>
	<div class="align-content-center">
		<button class="btn btn-success " asp-action="EditAlbum">Submit</button>
	</div>

</form>
